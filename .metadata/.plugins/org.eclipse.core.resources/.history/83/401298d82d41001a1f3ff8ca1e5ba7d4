package lv.kristina;

public class TestBookStore {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		char m = 'm';
		char f = 'f';
		char gender = 'j';
		
		
		if(gender == m || gender == f){
			System.out.println("It is M of F");
		}else{
			System.out.println("It is not M of F");
			
			
		}
	}

}
