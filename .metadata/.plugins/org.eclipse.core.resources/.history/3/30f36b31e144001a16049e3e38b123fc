package lv.kristina;

import lv.kristina.interfaces.Rectangle;
import lv.kristina.interfaces.Shape;
import lv.kristina.interfaces.Triangle;

public class TestInterface {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Shape figure = new Rectangle(5,10);
		
		
		System.out.println(figure.toString());
		System.out.println(figure.getArea());
		
		
		Shape figure1 = new Triangle(20,50);
		System.out.println(figure1.toString());
	}

}
