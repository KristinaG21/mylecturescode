
public class Calculator {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		char operatorMath = '+';
		
		//if(operatorMath == '/'){
		//	System.out.println("TRUE");
		//}else{
			//System.out.println("FALSE");
		
		double num1 = 20.0;
		double num2 = 24.5;
		double result;
		
		if(operatorMath == '+'){
			result = num1 + num2;
			System.out.println(result);
			
		}else if(operatorMath == '/'){
			result = num1 / num2;
			System.out.println(result);
		}else if(operatorMath == '*'){
			result = num1 * num2;
			System.out.println(result);
		}else if(operatorMath == '-'){
			result = num1 - num2;
			System.out.println(result);
		}else if(operatorMath == '%'){
			if(num1 >= num2);
			result = num1 % num2;
			System.out.println(result);
			
		}else{
			System.out.println("Wrong Data");
		}
		


	}

}
