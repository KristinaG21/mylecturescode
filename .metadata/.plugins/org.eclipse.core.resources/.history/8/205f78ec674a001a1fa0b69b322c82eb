	/**
	 * @param args
	 */
	public static void main(String[] args)throws Exception {
		File fileObj = new File("newfite.txt");
		if(fileObj.createNewFile()){
			System.out.println("File created: " +fileObj.getName());
		}else{
			System.out.println("File exists. ");
			
		}

	}

}
